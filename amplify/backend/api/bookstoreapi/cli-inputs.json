{
  "version": 1,
  "paths": {
    "/book": {
      "name": "/book",
      "lambdaFunction": "bookHandler",
      "permissions": {
        "setting": "open"
      }
    },
    "/generated-story/{book-id}": {
      "name": "/generated-story/{book-id}",
      "lambdaFunction": "generateStoryLambda",
      "permissions": {
        "setting": "open"
      }
    }
  }
}